{
  "expo": {
    "name": "hci-interior-app",
    "slug": "hci-interior-app",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
       "package": "gl.js.app",
      "permissions": [ "CAMERA", "WRITE_EXTERNAL_STORAGE", "CAMERA_ROLL" ],
      "versionCode": 36
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "university project",
    "githubUrl": "https://github.com/hci-interior"
  }
}
